<%#
  Renderiza o breadcrumb da página de Aplicativos do usuário.

  Variáveis:
    user: o usuário.
%>

<%= render "bootstrap/breadcrumb_mini", :breadcrumb_mini_items => [
  { :name => "Visão Geral",
    :link => home_user_path(user),
    :classes => "icon-home-lightblue_16_18-before text-replacement"
  },
  { :name => "Aplicativos",
    :link => user_oauth_clients_path(user),
    :classes => "icon-app-lightblue_16_18-before"
  }
  ] %>